{
  "name": "file-explorer",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron/main.js",
  "homepage": "./",
  "build": {
    "appId": "com.electron.yourappid",
    "files": [
      "build/**/*"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "1.3.4",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "debounce": "1.2.1",
    "electron-is-dev": "2.0.0",
    "filesize": "6.1.0",
    "framer-motion": "3.10.0",
    "promisify-child-process": "4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-virtualized-auto-sizer": "1.0.5",
    "react-window": "1.8.6"
  },
  "scripts": {
    "start": "concurrently \"set BROWSER=none && react-scripts start\" \"npm run electron\"",
    "build": "react-scripts build && electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "nodemon --watch public/electron --exec \"electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "6.0.0",
    "electron": "12.0.0",
    "electron-builder": "22.10.5",
    "electron-reload": "1.5.0",
    "nodemon": "2.0.7"
  }
}
